#include<stdio.h>
	void calculateresult(int m1, int m2, int m3){
		float avg = (m1 + m2 + m3) / 3.0;
		printf("Average= %.2f\n",avg);
		if(avg>=50 && m1>=40 && m2>=40 && m3>=40){
			printf("PASS\n");
		} else{
			printf("FAIL\n");
			if(avg < 50)
			printf("Average can not be less than 50 ");
			if(m1 < 40)
			printf("Subject 1 marks is less than 40");
			if(m2 < 40)
			printf("Subject 2 marks is less than 40");
			if(m1 < 40)
			printf("Subject 2 marks is less than 40");
		}
	}
int main(){
	int sub1,sub2,sub3;
	printf("Enter the marks of three subjects : \n");
	scanf("%d %d %d",&sub1,&sub2,&sub3);
	calculateresult(sub1,sub2,sub3);
	
	return 0;
}
