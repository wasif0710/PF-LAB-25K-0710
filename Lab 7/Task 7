#include <stdio.h>


int isDuplicate(int arr[], int index) {
    int i;
    for (i = 0; i < index; i++) {
        if (arr[i] == arr[index]) {
            return 1; 
        }
    }
    return 0; 
}

int main() {
    int arr[10];
    int i;

    printf("Enter 10 integers: ");
    for (i = 0; i < 10; i++) {
        scanf("%d", &arr[i]);
    }

    for (i = 0; i < 10; i++) {
        if (isDuplicate(arr, i)) {
            arr[i] = -1;  
        }
    }

    printf("Updated array: ");
    for (i = 0; i < 10; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    return 0;
}
